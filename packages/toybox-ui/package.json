    "antd": "^4.20.0",
    "classnames": "^2.3.1",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "react-is": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.intersection": "^4.4.6",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-grid-layout": "^1.1.1",
    "@umijs/plugin-sass": "^1.1.1",
