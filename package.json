{
  "name": "toy-box",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "version": "1.0.0",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build && lerna run build:tsc",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release-force": "lerna publish from-package",
    "release": "npm run build && lerna publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "clean": "lerna clean",
    "add": "lerna add"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ahooksjs/use-request": "^2.8.13",
    "@ahooksjs/use-table": "^0.2.11",
    "@airclass/icons": "^4.3.4",
    "ahooks": "^2.10.12",
    "antd": "^4.16.13",
    "immutability-helper": "^3.1.1",
    "loadsh": "0.0.4",
    "lodash.get": "^4.4.2",
    "lodash.intersection": "^4.4.0",
    "react": "^16.0.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^16.0.0",
    "react-grid-layout": "^1.3.0",
    "react-resizable": "^3.0.4",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@types/estree": "^0.0.48",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@types/jest": "^27.0.1",
    "@umijs/test": "^3.0.5",
    "babel-plugin-import": "^1.13.3",
    "dumi": "^1.1.31",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lerna": "^4.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "rollup-plugin-external-globals": "^0.6.1",
    "yorkie": "^2.0.0"
  }
}
